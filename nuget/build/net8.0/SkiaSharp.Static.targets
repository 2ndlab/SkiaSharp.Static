<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

    <ItemGroup>
        <None
            Include="$(MSBuildThisFileDirectory)..\..\runtimes\win-x64\native\libHarfBuzzSharp.lib"
            Link="runtimes\win-x64\native\libHarfBuzzSharp.lib"
            Condition="'$(RuntimeIdentifier)' == 'win-x64'">
        </None>
        <None
            Include="$(MSBuildThisFileDirectory)..\..\runtimes\win-x64\native\libHarfBuzzSharp.pdb"
            Condition="'$(RuntimeIdentifier)' == 'win-x64'">
        </None>
        <None
            Include="$(MSBuildThisFileDirectory)..\..\runtimes\win-x64\native\skia.lib"
            Link="runtimes\win-x64\native\skia.lib"
            Condition="'$(RuntimeIdentifier)' == 'win-x64'">
        </None>
        <None
            Include="$(MSBuildThisFileDirectory)..\..\runtimes\win-x64\native\SkiaSharp.lib"
            Link="runtimes\win-x64\native\SkiaSharp.lib"
            Condition="'$(RuntimeIdentifier)' == 'win-x64'">
        </None>
    </ItemGroup>

    <ItemGroup Condition="'$(PublishAot)' == 'true'">
        <DirectPInvoke Include="libHarfBuzzSharp" />
